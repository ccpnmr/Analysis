Dock QPushButton {
				color: #bec4f3;
				background-color: #535a83;
				border: 1px solid #182548;
				padding: 2px;
				}

Dock QPushButton::clicked {
                          color: #122043;
                          background-color: #BEC4F3;
                          border: 1px solid #182548;
                          padding: 2px;
                          }

Dock Frame {
          background-color: #2a3358;
          border: 1px solid #2a3358;
          padding: 1px;
}

Dock QToolButton {
          border: 0px solid #2a3358;
          padding: 0px;
}

Dock > QWidget QScrollArea { background-color: #2a3358;
        }

QDialog > QWidget QScrollArea { background-color: #2a3358;
        }

Dock > QWidget { background-color: #2a3358;
        }


Dock QPushButton::pressed {
                          color: #122043;
                          background-color: #BEC4F3;
                          border: 1px solid #182548;
                          padding: 2px;
                          }

Dock QPushButton::hover {
                          background-color: #e4e15b;
                          border: 1px solid #182548;
                          color: #122043;
                          padding: 2px;
                          }

Dock QPushButton::checked {
                           background-color: #182548;
                           padding: 2px;
                           }

GroupBox { background-color: #2a3358;}


QTreeWidget {
                  /* background-color: #020F31;*/
                  background-color: #0E1A3D;
}
QTreeWidget::branch {
                  background-color: #0E1A3D;
                  color: #e4e15b;
}
QTreeWidget::branch:closed {
                  background-color: #0E1A3D;
}

QTreeWidget::branch:closed:selected {
                  background-color: #666E98;
                  color: #F7FFFF;
}

QTreeWidget::branch:closed:hover {
                  background-color: #424A71;
                  color: #E4E15B;
                  }

QTreeWidget::branch:open:selected {
                  background-color: #666E98;
                  color: #F7FFFF;
}

QTreeWidget::branch:open:hover {
                  background-color: #424A71;
                  color: #E4E15B;
                  }

QTreeWidget::branch:closed:has-children {
                  background-color: #0E1A3D;
                  border-image: none;
                  image: url(:/dark-side.png);
                  padding-left: 10px;
}

QTreeWidget::branch:closed:has-children:selected {
                  background-color: #666E98;
                  color: #F7FFFF;
}

QTreeWidget::branch:closed:hover:has-children {
                  background-color: #424A71;
                  color: #E4E15B;
                  }

QTreeWidget::branch:open {
                  background-color: #0E1A3D;
                  border-image: none;
                  image: url(:/dark-down.png);
                  padding-left: 10px;
}


QTreeWidget::item {
                   color: #bec4f3;
                   background-color: #0E1A3D;

                   }
QTreeWidget::item::selected {
                  background-color: #666e98;
                  color: #f7ffff;
                  }
QTreeWidget::item::hover {
                  color: #e4e15b;
                  background-color: #424A71;
                  }

