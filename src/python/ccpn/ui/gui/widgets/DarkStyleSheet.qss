					}

QListWidget {background-color: #FDFDFC;
             color: #464E76;
             font-weight: normal;
             margin: 0px 0px 0px 0px;
             padding: 2px 2px 2px 2px;
             border: 0px solid #EDC151;
             font-weight: normal;
}

ObjectTable {
             border: 1px #182548;
             font-family: open-sans;
             alternate-background-color: black;

           }

ObjectTable::item {
                  padding: 2px;
                  background-color: #cccff9;
                  color: #122043;
                 }

ObjectTable::item::selected {
    background-color: #182548;
    color: #f7ffff;
}

TableWidget {
             border: 1px #182548;
             font-family: open-sans;
             alternate-background-color: black;

           }

TableWidget::item {
                  padding: 2px;
                  background-color: #cccff9;
                  color: #122043;
                 }

TableWidget::item::selected {
    background-color: #182548;
    color: #f7ffff;
}

/* setting the QHeader background colour produces an unexpected behavior when a row is selected. */

