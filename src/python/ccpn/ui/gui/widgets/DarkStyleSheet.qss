QWidget {
    padding: 0;
    margin: 0px 0px 0px 0px;
    border: 0px;
}

QFrame {
    background-color: #2a3358;
    border: 0px solid #f7ffff;
}

QTextEdit {
        background-color: #f7ffff;
        color: #122043;
        margin: 0px 0px 0px 0px;
        border: 1px solid #182548;
        font-size: 1.5em;
        }

QLabel {
    padding: 0;
    background-color: #2a3358;
    color: #2a3358;
    border: 0px;
}

CallTipWidget {
    color: #bec4f3;
}

GroupBox#borderedGroupBox {
border: 1px solid #bec4f3;
}

QWidget#spectrumSelectionWidget {background-color: #2a3358;}

QAbstractItemView#CompleterList {
background-color: #f7ffff;
}


QColorDialog {
    background-color: #2a3358;
}

QDialog {
        background-color: #2a3358;
        color: #f7ffff;
        }

QMenu {
      background-color: #424a71;
    }

QMenu::right-arrow {
    border-image: none;
    image: url(:/dark-side.png);
}

QMenuBar {
              background-color: #424a71;
              /* font-size: 1.5em; */
              /* font-weight: bold; */
              padding: 2px;
              /* border-bottom: 1px solid #00092d; */
              }

QMessageBox QPushButton {
    padding: 10px;
    border: 1px solid #182548;
}

DoubleSpinbox {
              background-color: #f7ffff;
              color: #122043;
              border: 0px solid #EDC151;
              }

DoubleSpinbox::hover {
              background-color: #e4e15b;
              }

Spinbox {
              background-color: #f7ffff;
              border: 0px solid #EDC151;
              color: #122043;
              }

Spinbox::hover {
              background-color: #e4e15b;
              }

RadioButton {
              background-color: #2a3358;
              color: #f7ffff;
              font-weight: bold
              }

RadioButton::disabled {
                      color: #7f88ac
                      }

/* QComboBox /PulldownList padding appears not to work; hardcoded instead */
QComboBox {
    padding-top: 2px;
    padding-bottom: 2px;
    padding-left: 2px;
}

QComboBox QAbstractItemView::item {
          padding-top: 3px;
          padding-bottom: 3px;
          padding-left: 1px;
          padding-right: 1px;
}

PulldownList {
          background-color: #f7ffff;
          border: 0px solid #182548;
          color: #122043;
          margin-left: 4px;
          padding-left: 2px;
          font-weight: normal;
          }

PulldownList::item {
                   background-color: #f7ffff;
                   color: #122043;
                   }
PulldownList::item::selected {
                   background-color: #e4e15b;
                   color: #122043;
                   }

PulldownList QListView { background-color: #f7ffff; }

PulldownList::hover {
                  background-color: #e4e15b;
                  color: #122043;
                  }

QMenuBar::item {
                background-color: #424a71;
                color: #bec4f3;

              }
QMenuBar::item::selected { color: #f7ffff;
                        background: #666e98;
                      }
QMenuBar::item::hover {
     background-color: #666E98;
     color: #e4e15b;
      }

QMenu::item {background-color: #424a71;
             color: #bec4f3;
              }
QMenu::item::selected { color: #f7ffff;
                        background-color: #666e98;
                      }

QMenu::item::disabled {background-color: #424a71;
             color: #7f88ac;
              }

QMenu::item::disabled::selected {background-color: #424a71;
             color: #7f88ac;
              }

