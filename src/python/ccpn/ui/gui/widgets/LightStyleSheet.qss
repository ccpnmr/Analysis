Dock {
         background-color: #FBF4CC;
         border: 0px solid #FBF4CC;
}

Dock DockLabel  {
          background-color: #8087B3;
          color: #FDFDFC;
          border: 1px solid #fdfdfc;
          padding: 0px;
          /*font-weight: Semibold;*/
}

Dock QPushButton {
				color: #fdfdfc;
				background-color: #bd8413;
				border: 1px solid #EDC151;
				padding: 2px;
				}

Dock QPushButton::clicked {
                          color: #122043;
                          background-color: #BEC4F3;
                          border: 1px solid #182548;
                          padding: 2px;
                          }

Dock Frame {
          background-color: #FBF4CC;
          border: 1px solid #FBF4CC;
          padding: 1px;
}

