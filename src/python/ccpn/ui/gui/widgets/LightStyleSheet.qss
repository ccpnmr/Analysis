          border: 0px solid #EDC151;
          padding: 0px;
}

Dock > QWidget QScrollArea { background-color: #FBF4CC;
                             padding: 0px;
        }

QDialog > QWidget QScrollArea { background-color: #FBF4CC;
        }

Dock > QWidget { background-color: #FBF4CC;
                 padding: 0px;
        }

Dock QPushButton::pressed {
                          color: #fdfdfc;
                          background-color: #8087b3;
                          border: 1px solid #8087b3;
                          padding: 2px;
                          }

Dock QPushButton::hover {
                          background-color: #009a00;
                          border: 1px solid #009a00;
                          color: #fdfdfc;
                          padding: 2px;
                          }

Dock  QPushButton::checked {
				color: #fdfdfc;
				background-color: #8087b3;
				padding: 2px;
				}

GroupBox { background-color: #EDC151;}


QTreeWidget {
                  background-color: #EDC151;
}
QTreeWidget::branch {
                  background-color: #EDC151;
                  /*border-top: 1px solid #EDC151;*/
                  /*border-bottom: 1px solid #EDC151;*/
}

QTreeWidget::branch:closed {
                  background-color: #EDC151;
}

QTreeWidget::branch:closed::selected {
                  background-color: #009A00;
                  color: #FDFDFC;
}

QTreeWidget::branch:closed::hover {
                  background-color: #FFFCBA;
                  color: #009A00;
}

QTreeWidget::branch:open::selected {
                  background-color: #009A00;
                  color: #FDFDFC;
}

QTreeWidget::branch:open::hover {
                  background-color: #FFFCBA;
                  color: #009A00;
}

QTreeWidget::branch:closed:has-children {
                  background-color: #EDC151;
                  border-image: none;
                  image: url(:/light-side.png);
                  padding-left: 10px;
}

QTreeWidget::branch:closed:has-children:selected {
                  background-color: #009A00;
                  color: #FDFDFC;
}

QTreeWidget::branch:closed:hover:has-children {
                  background-color: #FFFCBA;
                  color: #009A00;
                  }

QTreeWidget::branch:open {
                  background-color: #EDC151;
                  border-image: none;
                  image: url(:/light-down.png);
                  padding-left: 10px;
}


QTreeWidget::item {
                  color: #555D85;
                  background-color: #EDC151;
                   }
QTreeWidget::item::selected {
                  background-color: #009A00;
                  color: #FDFDFC;
                  }
QTreeWidget::item::hover {
                  color: #009A00;
                  background-color: #FFFCBA;
                  }

