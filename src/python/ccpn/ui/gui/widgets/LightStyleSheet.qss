QWidget {
    padding: 0;
    margin: 0px 0px 0px 0px;
    border: 0px;
}

QFrame {
    background-color: #FBF4CC;
    border: 0px solid #FBF4CC;
}

QTextEdit {
        background-color: #f7ffff;
        color: #122043;
        margin: 0px 0px 0px 0px;
        border: 1px solid #f7ffff;
        font-size: 1.5em;
        }

QLabel {
    padding: 0;
    background-color: #FBF4CC;
    color: #FBF4CC;
    border: 0px;
}

CallTipWidget {
    color: #555D85;
}

GroupBox#borderedGroupBox {
border: 1px solid #EDDFBC;
}

QWidget#spectrumSelectionWidget {background-color: #FBF4CC;}

