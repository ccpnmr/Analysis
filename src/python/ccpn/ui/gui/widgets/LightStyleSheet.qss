                      }

/* QComboBox /PulldownList padding appears not to work; hardcoded instead */
QComboBox {
    padding-top: 2px;
    padding-bottom: 2px;
    padding-left: 2px;
}

QComboBox QAbstractItemView::item {
          padding-top: 3px;
          padding-bottom: 3px;
          padding-left: 1px;
          padding-right: 1px;
}

PulldownList {
          background-color: #f7ffff;
          border: 0px solid #EDC151;
          color: #464E76;
          margin-left: 4px;
          padding-left: 2px;
          font-weight: normal;
          }

PulldownList::item {
                   /* background-color: #fffdd1; */
                   color: #464e76;
                   }
PulldownList::item::selected {
                   background-color: #fdfdfc;
                   color: #464e76;
                   }

PulldownList QListView { background-color: #f7ffff; }

PulldownList::hover {
                  background-color: #f7ffff;
                  color: #464E76;
                  }

QMenuBar::item {
                background-color: #EDDFBC;
                color: #555D85;
              }
QMenuBar::item::selected { color: #FDFDFC;
                        background: #009A00;
                      }

